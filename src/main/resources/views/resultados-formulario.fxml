<?xml version="1.0" encoding="UTF-8"?>

<?import javafx.scene.control.Button?>
<?import javafx.scene.control.Label?>
<?import javafx.scene.control.TableColumn?>
<?import javafx.scene.control.TableView?>
<?import javafx.scene.layout.AnchorPane?>

<AnchorPane prefHeight="400.0" prefWidth="600.0" stylesheets="@../style/style.css" xmlns="http://javafx.com/javafx/20.0.1" xmlns:fx="http://javafx.com/fxml/1" fx:controller="com.example.alcanzandonota.controller.ResultadosFormularioController">
   <children>
      <Label id="title" layoutX="186.0" layoutY="14.0" text="Resultados de la Prueba" />
      <Button id="text" fx:id="btnAceptar" layoutX="266.0" layoutY="360.0" mnemonicParsing="false" onAction="#aceptarAction" text="Aceptar" />
      <Label id="text" fx:id="lblPuntaje" layoutX="271.0" layoutY="68.0" text="Puntaje 0" />
      <TableView fx:id="tblResultados" layoutX="200.0" layoutY="100.0" prefHeight="200.0" prefWidth="200.0">
        <columns>
          <TableColumn fx:id="colPregunta" prefWidth="75.0" text="Pregunta" />
            <TableColumn fx:id="colValor" prefWidth="75.0" text="Valor" />
            <TableColumn fx:id="colEstado" prefWidth="75.0" text="Estado" />
        </columns>
         <columnResizePolicy>
            <TableView fx:constant="CONSTRAINED_RESIZE_POLICY" />
         </columnResizePolicy>
      </TableView>
   </children>
</AnchorPane>
